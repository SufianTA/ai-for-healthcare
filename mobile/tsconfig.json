{
  "compilerOptions": {
    "allowJs": false,
    "jsx": "react-native",
    "strict": true,
    "noImplicitAny": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "target": "es2017",
    "lib": ["es2017"],
    "types": ["react", "react-native"],
    "skipLibCheck": true
  },
  "extends": "expo/tsconfig.base"
}
